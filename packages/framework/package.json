{
  "name": "@web3-savings-cards/framework-react",
  "author": "kames",
  "version": "0.0.0-beta.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/identity-framework.esm.js",
  "contributors": [
    {
      "name": "Kames Geraghty",
      "email": "dev@kames.me"
    }
  ],
  "scripts": {
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "watch": "tsdx watch",
    "package:build": "tsdx build",
    "package:lint": "tsdx lint",
    "package:test": "tsdx test",
    "package:watch": "tsdx watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .cache",
    "clean:test": "jest --clearCache",
    "docs": "yarn typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true --namedAnchors true --entryDocument index.md",
    "docs:html": "yarn typedoc --plugin none --out docs",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "size-limit": [
    {
      "path": "dist/identity-framework.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/identity-framework.esm.js",
      "limit": "10 KB"
    }
  ],
  "files": [
    "assets",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "@babel/core": "7.18.5",
    "@babel/preset-env": "^7.19.4",
    "@rainbow-me/rainbowkit": "0.4.6",
    "ethers": "5.5.1",
    "react": ">=16",
    "wagmi": "0.6.3"
  },
  "dependencies": {
    "@web3-savings-cards/core-sol": "0.0.0-beta.0",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.1",
    "@ethersproject/transactions": "^5.5.0",
    "@turbo-eth/core-wagmi": "0.0.0-beta.2",
    "@types/react-query": "^1.2.9",
    "classnames": "^2.3.1",
    "next": "^12.1.6",
    "prop-types": "15.8.1",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.27.1",
    "react-modal-hook": "^3.0.0",
    "react-query": "^3.39.2",
    "react-responsive": "^9.0.0",
    "react-select": "^5.3.2",
    "react-spring": "^9.5.5",
    "react-toggle-button": "^2.2.0",
    "recoil": "^0.7.5",
    "tailwindcss": "^3.1.6",
    "token-icons": "0.1.0-alpha.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.7",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
